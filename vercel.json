{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/auth/facebook/callback",
      "destination": "/api/facebook/callback"
    },
    {
      "source": "/auth/facebook/auth",
      "destination": "/api/facebook/auth"
    },
    {
      "source": "/auth/ebay/callback",
      "destination": "/api/ebay/callback"
    },
    {
      "source": "/auth/ebay/auth",
      "destination": "/api/ebay/auth"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
